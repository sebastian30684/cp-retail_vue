{
    "title": "ServiceTicketSubmitted",
    "type": "object",
    "description": "Event fired when a customer submits a service ticket",
    "properties": {
        "cookieId": {
            "title": "Cookie ID",
            "type": "string",
            "description": "Browser cookie identifier for anonymous tracking"
        },
        "cdcUid": {
            "title": "CDC User ID",
            "type": "string",
            "description": "Gigya CDC user identifier"
        },
        "timestamp": {
            "title": "Timestamp",
            "type": "string",
            "description": "ISO 8601 timestamp of event"
        },
        "pageUrl": {
            "title": "Page URL",
            "type": "string",
            "description": "URL where ticket was submitted"
        },
        "websiteDomain": {
            "title": "Website Domain",
            "type": "string",
            "description": "Domain hostname"
        },
        "organizationId": {
            "title": "Organization ID",
            "type": "string",
            "description": "Organization identifier (sap-cx-retail)"
        },
        "ticketId": {
            "title": "Ticket ID",
            "type": "string",
            "description": "Generated ticket identifier (TKT-{timestamp}-{random})"
        },
        "firstName": {
            "title": "First Name",
            "type": "string",
            "description": "Customer first name (guest users only)"
        },
        "lastName": {
            "title": "Last Name",
            "type": "string",
            "description": "Customer last name (guest users only)"
        },
        "primaryEmail": {
            "title": "Primary Email",
            "type": "string",
            "description": "Customer email address (guest users only)"
        },
        "phone": {
            "title": "Phone",
            "type": "string",
            "description": "Customer phone number (optional, guest users only)"
        },
        "category": {
            "title": "Category",
            "type": "string",
            "description": "Service category (buying-fitting-advice, repairs-servicing, test-bikes, bike-rental, collect-ride, new-bike-day, new-bike-pickup, suspension-servicing, wheel-building, mobile-repairs, bike-fitting, velofix, bike-boxing-shipping, ebike-battery-disposal)"
        },
        "priority": {
            "title": "Priority",
            "type": "string",
            "description": "Ticket priority level (low, medium, high)"
        },
        "subject": {
            "title": "Subject",
            "type": "string",
            "description": "Brief description of the issue"
        },
        "description": {
            "title": "Description",
            "type": "string",
            "description": "Detailed description of the issue"
        },
        "bike_id": {
            "title": "Bike ID",
            "type": "string",
            "description": "Optional: accountID of the selected bike from CDP Bike Garage"
        },
        "bikeName": {
            "title": "Bike Name",
            "type": "string",
            "description": "Optional: accountName of the selected bike from CDP Bike Garage"
        },
        "ticketStatus": {
            "title": "Ticket Status",
            "type": "string",
            "description": "Current status of ticket (open, in_progress, resolved, closed)"
        }
    }
}
